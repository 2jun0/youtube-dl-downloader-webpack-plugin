{
  "name": "youtube-dl-downloader-wepback-plugin",
  "version": "1.0.15",
  "description": "A webpack plugin that downloads youtube-dl",
  "files": [
    "dist/"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "npm run build:babel && npm run build:types",
    "build:types": "tsc --project tsconfig.types.json",
