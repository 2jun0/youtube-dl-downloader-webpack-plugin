    "typescript": "tsc",
    "prepublishOnly": "npm run build && npm run typescript"
  },
  "author": "2jun0 <soo28819@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/2jun0/youtube-dl-downloader-webpack-plugin.git"
  },
  "keywords": [
    "webpack",
    "plugin",
    "youtube-dl",
    "downloader"
  ],
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.0",
    "@babel/preset-env": "^7.16.4",
    "@babel/preset-typescript": "^7.16.0",
    "@types/node": "^16.11.11",
    "prettier": "^2.5.1",
    "typescript": "^4.5.2",
    "webpack": "^5.64.4"
  },
  "license": "MIT"
}
